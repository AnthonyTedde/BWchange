	file = {Snapshot:/home/ate/Zotero/storage/FUWD7RKR/fulltext.html:text/html;Full Text PDF:/home/ate/Zotero/storage/JAVAS7HA/Zaalberg et al. - 2019 - Genetic analysis of Fourier transform infrared mil.pdf:application/pdf},
}

